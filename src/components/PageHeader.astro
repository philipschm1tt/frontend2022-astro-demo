---
export interface Props {
    headline: string,
    subHeadline: string,
}
const {headline, subHeadline} = Astro.props;
---
<header class="py-20 px-4 bg-blue-900">
	<div class="container mx-auto px-4">
		<h1 class="text-4xl font-bold mb-2 text-white">
			{headline}
		</h1>
		<p class="text-2xl text-gray-200">
			{subHeadline}
		</p>
	</div>
</header>
